package templ

import "github.com/ViBiOh/commute/pkg/model"
import "strconv"

templ Result(uri, nonce, title, staticImageURL string, commutes model.Commutes) {
  @Root(uri, nonce, title) {
    <img src={ staticImageURL } class="max-full" alt="Static image of home and work"/>
    <pre>
      { commutes.String() }
    </pre>
    <p>
      Total: { strconv.Itoa(len(commutes)) }
    </p>
  }
}
